{
  "name": "h5p-boilerplate",
  "version": "1.0.0",
  "description": "Example H5P Content Type for getting started",
  "private": true,
  "scripts": {
    "test": "ava",
    "build": "cross-env NODE_ENV='production' webpack -p",
