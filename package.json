{
  "name": "h5p-boilerplate",
  "version": "1.0.0",
  "description": "Example H5P Content Type for getting started",
  "private": true,
  "scripts": {
    "webpack:b": "cross-env NODE_ENV='production' webpack -p",
    "webpack:w": "webpack --watch",
    "scss:w": "node-sass --watch src/styles/scss -o src/styles/css",
    "scss:b": "node-sass src/styles/scss -o src/styles/css",
    "build": "npm run scss:b && npm run webpack:b",
    "dev-win": "start npm run webpack:w && start npm run scss:w"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/h5p/h5p-boilerplate.git"
  },
  "keywords": [
    "h5p",
    "library"
  ],
  "author": "Olav Daling Stornes",
  "license": "MIT",
  "devDependencies": {
    "ava": "^0.25.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^6.3.2",
    "babel-plugin-transform-object-assign": "^6.22.0",
    "babel-preset-latest": "^6.22.0",
    "cross-env": "^4.0.0",
    "css-loader": "^0.26.2",
    "file-loader": "^2.0.0",
    "style-loader": "^0.13.2",
    "webpack": "^2.7.0"
  },
  "dependencies": {
    "node-sass": "^4.9.3",
    "url-loader": "^1.1.2"
  }
}
