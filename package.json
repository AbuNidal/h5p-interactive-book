  "license": "MIT",
  "devDependencies": {
    "ava": "^0.25.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^6.3.2",
    "babel-plugin-transform-object-assign": "^6.22.0",
    "babel-preset-latest": "^6.22.0",
    "cross-env": "^4.0.0",
    "css-loader": "^0.26.2",
    "file-loader": "^2.0.0",
    "style-loader": "^0.13.2",
    "webpack": "^2.7.0"
  },
  "dependencies": {
    "node-sass": "^4.9.3",
    "url-loader": "^1.1.2"
